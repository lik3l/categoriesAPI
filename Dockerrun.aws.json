{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "python-app",
      "host": {
        "sourcePath": "."
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "python-app",
      "image": "lik3l/categories-api",
      "environment": {
        "name": "DEBUG",
        "value": 1
      },
      "essential": true,
      "memory": 128,
      "mountPoints": [
        {
          "sourceVolume": "python-app",
          "containerPath": "/code"
        }
      ],
      "portMappings": [
        {
          "hostPort": 8000,
          "containerPort": 8000
        }
      ]
    }
  ]
}